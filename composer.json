{
    "name": "notifylog/laravel",
    "description": "Real-time event notification",
    "keywords": [
        "monitoring",
        "php",
        "notifylog",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mauro Marchiori",
            "email": "mauro@mogiana.co"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1"
    },
    "require-dev": {
        "laravel/framework": "^8|^9"
    },
    "autoload": {
        "psr-4": {
            "NotifyLog\\Laravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload-dev": {
        "psr-4": {
            "NotifyLog\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotifyLog\\Laravel\\NotifyLogServiceProvider"
            ]
        }
    }
}
